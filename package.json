{
  "name": "dharmamitra-frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "set-variant": "sh ./scripts/set_build_variant.sh",
    "build:pub": "sh ./scripts/set_build_variant.sh pub && next build",
    "build:lab": "sh ./scripts/set_build_variant.sh lab && next build",
    "build:rnd": "sh ./scripts/set_build_variant.sh rnd && next build",
    "build:kumarajiva": "sh ./scripts/set_build_variant.sh kumarajiva && next build",
    "build": "next build",
    "start": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && node .next/standalone/server.js",
    "test:ts": "tsc --noEmit",
    "test:pw": "playwright test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:strict": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write */**/*.{js,jsx,json,ts,tsx,scss,css,md}",
    "i18n:modelsync": "python3 ./scripts/sync_msg_model.py",
    "prepare": "husky",
    "api:ts": "openapi-typescript && eslint src/lib/api/*.ts --fix"
  },
  "hooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.28",
    "@ai-sdk/react": "^2.0.104",
