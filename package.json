{
  "name": "dharmamitra-frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:pub": "sh ./scripts/set_env.sh pub && next build",
    "build:lab": "sh ./scripts/set_env.sh lab && next build",
    "build:rnd": "sh ./scripts/set_env.sh rnd &&next build",
    "build:kumarajiva": "sh ./scripts/set_env.sh kumarajiva && next build",
    "build": "next build",
    "start": "next start",
    "test:ts": "tsc --noEmit",
    "test:pw": "playwright test",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint '*/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write */**/*.{js,jsx,json,ts,tsx,scss,css,md}",
    "prepare": "husky",
    "api:codegen": "openapi-typescript https://dharmamitra.org/api/openapi.json -o ./src/lib/api/translation.v1.d.ts && openapi-typescript https://dharmamitra.org/api-search/openapi.json -o ./src/lib/api/search.v1.d.ts",
    "api:codegen:translation": "openapi-typescript https://dharmamitra.org/api/openapi.json -o ./src/lib/api/translation.v1.d.ts",
    "api:codegen:search": "openapi-typescript https://dharmamitra.org/api-search/openapi.json -o ./src/lib/api/search.v1.d.ts",
    "bn-temp-api:codegen": "openapi-typescript https://dharmamitra.org/api-db/openapi.json -o ./src/lib/api/bnv2.d.ts"
  },
  "hooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/material-nextjs": "^5.15.11",
    "@tanstack/react-query": "^5.45.0",
    "@tanstack/react-query-devtools": "^5.45.0",
    "axe-core": "^4.9.1",
    "axe-html-reporter": "^2.2.3",
    "dotenv": "^16.4.5",
    "jotai": "^2.8.3",
    "next": "14.2.4",
    "next-intl": "^3.15.0",
    "openapi-fetch": "^0.9.7",
    "playwright-core": "^1.44.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.10.3",
    "react-window": "^1.8.10",
    "sharp": "^0.33.4",
    "sse.js": "^2.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@playwright/test": "^1.44.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@tanstack/eslint-plugin-query": "^5.43.1",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "globals": "^15.5.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "openapi-typescript": "^6.7.6",
    "prettier": "^3.3.2",
    "semantic-release": "^23.1.1",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.3.0"
}
